{
    "env": {
        "production": {
            "presets": [
                ["@babel/preset-env", {
                    "targets": {
                        "browsers": ["last 2 version", "ie 10"]
                    },
                    "modules": false,
                    "debug": false
                }], "@babel/preset-react"
            ],
            "plugins": [
                "@babel/plugin-transform-runtime",
                "@babel/plugin-proposal-object-rest-spread",
                ["@babel/plugin-proposal-decorators", { "legacy": true }],
                ["@babel/plugin-proposal-class-properties", { "loose": true }]
            ]
        },
        "development": {
            "presets": [
                ["@babel/preset-env", {
                    "targets": {
                        "chrome": 52
                    },
                    "modules": false,
                    "debug": false
                }], "@babel/preset-react"
            ],
            "plugins": [
                "@babel/plugin-proposal-object-rest-spread",
                ["@babel/plugin-proposal-decorators", { "legacy": true }],
                ["@babel/plugin-proposal-class-properties", { "loose": true }]
            ]
        }
    }
}
